<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
                <block class="Magento\Framework\View\Element\Html\Link" name="custom-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Contact</argument>
                    <argument name="path" xsi:type="string" translate="true">contact</argument>
                </arguments>
                </block>
        </referenceBlock>
        <!--<referenceContainer name="header.panel">
            <block class="Magento\Framework\View\Element\Html\Links" name="header.links">
                <arguments>
                    <argument name="css_class" xsi:type="string">header links</argument>
                </arguments>
            </block>
        </referenceContainer>-->
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file"  xsi:type="string">web/images/logo.svg</argument>
                <argument name="logo_img_width" xsi:type="number">200</argument>
                <argument name="logo_img_height" xsi:type="number">80</argument>
            </arguments>
        </referenceBlock>
        <!--<referenceContainer name="footer">
            <block class="Magento\Store\Block\Switcher" name="store_switcher" as="store_switcher" after="footer_links" template="Magento_Store::switch/stores.phtml"/>
        </referenceContainer>-->
        <referenceBlock name="footer_links">
        <block class="Magento\Framework\View\Element\Html\Link\Current" name="suggest-us-link">
            <arguments>
                <argument name="label" xsi:type="string">Please Suggest Us</argument>
                <argument name="path" xsi:type="string">crud/insert/index</argument>
            </arguments>
        </block>
        </referenceBlock>
        <referenceBlock name="report.bugs" remove="true"/>
        <referenceBlock name="copyright" remove="true"/> 
    </body>
</page>